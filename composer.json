{
    "name": "pagekit/pagekit",
    "version": "0.8.0",
    "type": "project",
    "description": "The Pagekit CMS",
    "keywords": ["cms", "pagekit"],
    "homepage": "http://pagekit.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Pagekit Team",
            "email": "info@pagekit.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "1.0.22",
        "doctrine/annotations": "~1.2.3",
        "doctrine/dbal": "~2.5.1",
        "guzzlehttp/guzzle": "~5.3.0",
        "ircmaxell/password-compat": "~1.0.0",
        "ircmaxell/random-lib": "~1.0.0",
        "lusitanian/oauth": "~0.3.5",
        "maximebf/debugbar": "~1.10.4",
        "monolog/monolog": "~1.13",
        "nikic/php-parser": "~1.2.2",
        "psr/log": "~1.0.0",
        "punic/punic": "~1.5.0",
        "swiftmailer/swiftmailer": "~5.4.1",
        "symfony/console": "~2.7.1",
        "symfony/debug": "~2.7.1",
        "symfony/finder": "~2.7.1",
        "symfony/http-foundation": "~2.7.1",
        "symfony/routing": "~2.7.1",
        "symfony/stopwatch": "~2.7.1",
        "symfony/templating": "~2.7.1",
        "symfony/translation": "~2.7.1",
        "symfony/var-dumper": "~2.7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.3"
    },
    "repositories": [
        {
          "type": "artifact",
          "url": "tmp/packages/"
        },
        {
          "type": "composer",
          "url": "https://pagekit.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pagekit\\": "app/modules/application/src",
            "Pagekit\\Config\\": "app/modules/config/src",
            "Pagekit\\Updater\\": "app/updater/src"
        }
    },
    "scripts": {
        "pre-update-cmd": "Pagekit\\Updater\\ComposerEvent::extend",
        "pre-install-cmd": "Pagekit\\Updater\\ComposerEvent::extend",
        "pre-status-cmd": "Pagekit\\Updater\\ComposerEvent::extend"
    }
}
